{
  "manifest_version": 3,
  "name": "TechInAsia Content Unlocker",
  "version": "1.2",
  "author": "Phu Duc Nguyen",
  "description": "Unlock paywalled content on TechInAsia articles",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "icons": {
    "48": "icon.png"
  },
  "action": {
    "default_title": "Click to Read Content",
    "default_icon": {
      "48": "icon.png"
    }
  },
  "host_permissions": [
    "https://freetia-mochfamir.vercel.app/*"
  ],
  "background": {
    "service_worker": "background.js"
  }
}